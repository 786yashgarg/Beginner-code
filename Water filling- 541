#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin >> t;
    while (t > 0)
    {
        t--;
        int x, y, z;
        cin >> x >> y >> z;
        if (x && y == 1 || y && z == 1 || z && x == 1)
            cout << "not now" << endl;
        else
            cout << "water filling time " << endl;
    }
    return 0;

}
