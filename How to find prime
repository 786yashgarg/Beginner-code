#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        int p = 0;
        cin >> n;
        for (int i = 1; i <= n; i++)

            if (n % i == 0)
                p++;

        if (p > 2)
            cout << "no" << endl;
        else if (p == 2)
            cout << "yes" << endl;
        else
            cout << "no" << endl;
    }




    return 0;

}
